{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-unsafe-optional-chaining": "off",
        "no-console": "warn",
        "camelcase": "warn"
    }
}
