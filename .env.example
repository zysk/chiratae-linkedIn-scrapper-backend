# Server Configuration
PORT=4000
NODE_ENV=development

# MongoDB Connection String
MONGOURI=mongodb://localhost:27017/linkedin-scraper

# JWT Configuration
JWT_ACCESS_TOKEN_SECRET=your_jwt_secret
JWT_REFRESH_TOKEN_SECRET=your_jwt_refresh_secret
JWT_ACCESS_TOKEN_EXPIRY=1d
JWT_REFRESH_TOKEN_EXPIRY=7d

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_PREFIX=linkedin-scraper:
REDIS_KEY_EXPIRY=172800

# Job Queue Configuration
JOB_QUEUE_PREFIX=linkedin-scraper:jobs:
JOB_CONCURRENCY=2
JOB_RETRY_DELAY=60000
JOB_RETRY_ATTEMPTS=3
JOB_STALLED_INTERVAL=30000
JOB_BACKOFF_STRATEGY=exponential

# Scheduler Configuration
SCHEDULER_ENABLED=true
SCHEDULER_PREFIX=linkedin-scraper:schedules:
SCHEDULER_TIMEZONE=Asia/Kolkata

# LinkedIn Scraper Configuration
ENABLE_CRON=false
HEADLESS_BROWSER=false
TZ=Asia/Kolkata

# Email Configuration
EMAIL_HOST=smtp.example.com
EMAIL_PORT=587
EMAIL_USER=your_email@example.com
EMAIL_PASS=your_email_password
EMAIL_FROM=your_email@example.com
