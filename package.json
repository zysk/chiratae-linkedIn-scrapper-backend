{
  "name": "linkedin-scraper-backend",
  "version": "1.0.0",
  "description": "LinkedIn lead generation and scraping tool - TypeScript rewrite",
  "main": "dist/bin/www.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/bin/www.js",
    "dev": "nodemon --watch src --exec ts-node src/bin/www.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --format=codeframe",
    "fix:line-endings": "git ls-files -z | xargs -0 git add --renormalize",
    "clean-build": "rm -rf node_modules dist package-lock.json && npm install && npm run build",
    "type-check": "node scripts/type-check.js",
    "fix:console-logs": "node scripts/fix-console-logs.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "tssync": "typesync"
  },
  "keywords": [
    "linkedin",
    "scraper",
    "typescript",
    "selenium"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "chromium": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.14.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "nanoid": "^5.1.5",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.10.1",
    "qr-image": "^3.2.0",
    "qrcode": "^1.5.4",
    "redis": "^5.0.0",
    "selenium-webdriver": "^4.31.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/eslint": "^9.6.1",
    "@types/express": "^5.0.1",
    "@types/http-errors": "~2.0.4",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.3",
    "@types/node-schedule": "^2.1.7",
    "@types/nodemailer": "^6.4.17",
    "@types/qr-image": "^3.2.9",
    "@types/qrcode": "^1.5.5",
    "@types/selenium-webdriver": "^4.1.28",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typesync": "^0.14.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/bin/*.{ts,tsx}",
      "!**/node_modules/**"
    ]
  }
}
