{
  "name": "chiratae-linkedin-scraper-backend",
  "version": "1.0.0",
  "description": "Backend API for LinkedIn Scraper",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "change-port": "node scripts/change-port.js",
    "port-manager": "node scripts/port-manager.js",
    "port:check": "node scripts/port-manager.js check",
    "port:find": "node scripts/port-manager.js find",
    "port:list": "node scripts/port-manager.js list",
    "dev:port": "cross-env PORT=$npm_config_port nodemon src/server.ts",
    "start:port": "cross-env PORT=$npm_config_port node dist/server.js",
    "dev:4001": "cross-env PORT=4001 nodemon src/server.ts",
    "dev:4002": "cross-env PORT=4002 nodemon src/server.ts",
    "test:linkedin-proxy": "node scripts/test-runner.js",
    "test:linkedin-proxy:port": "node scripts/test-runner.js --port"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.7",
    "redis": "^4.6.10",
    "selenium-webdriver": "^4.15.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.5",
    "@types/cookie-parser": "^1.4.5",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/http-errors": "^2.0.3",
    "@types/jest": "^29.5.6",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/morgan": "^1.9.7",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.8.10",
    "@types/node-schedule": "^2.1.2",
    "@types/nodemailer": "^6.4.13",
    "@types/selenium-webdriver": "^4.1.19",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "axios": "^1.9.0",
    "commander": "^13.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
